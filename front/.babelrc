{
  "presets": [
    [
      "next/babel",
      {
        "preset-react": {
          "runtime": "automatic",
          "importSource": "@emotion/react"
        }
      }
    ]
  ],
  "plugins": [
    //...
    // ["styled-components", { "ssr": true }],
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@store": "./src/store",
          "@utils": "./src/utils",
          "@hooks": "./src/hooks",
          "@components": "./src/components",
          "@layouts": "./src/layouts",
          "@typings": "./src/typings",
          "@pages": "./pages"
        }
      }
    ]
  ]
}
